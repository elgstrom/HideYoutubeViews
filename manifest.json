{
    "manifest_version": 2,
    "name": "Hide YouTube Views",
    "version": "1.0",
    "description": "Hides the view counts on YouTube videos.",
    "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["contentScript.js"],
        "run_at": "document_end"
      }
    ]
  }
  